{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "appData": {
    "version": "1.2.6",
    "timestamp": "2026-01-26T09:20:00Z",
    "description": "Exclude all API calls from service worker cache"
  },
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
  {
    "name": "static-api",
    "urls": [
      "https://api.xdealhub.com/utils/countries",
      "https://api.xdealhub.com/utils/industries"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 20,
      "maxAge": "7d",
      "timeout": "10s"
    }
  },
  {
    "name": "user-content",
    "urls": [
      "https://api.xdealhub.com/files/**"
    ],
    "cacheConfig": {
      "strategy": "performance",
      "maxSize": 50,
      "maxAge": "2d",
      "timeout": "2s"
    }
  },
  {
    "name": "google-fonts",
    "urls": [
      "https://fonts.googleapis.com/**",
      "https://fonts.gstatic.com/**"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 30,
      "maxAge": "30d",
      "timeout": "2s"
    }
  },
  {
    "name": "external-libraries",
    "urls": [
      "https://cdnjs.cloudflare.com/**"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 10,
      "maxAge": "30d",
      "timeout": "2s"
    }
  }
 ],
 "navigationRequestStrategy": "freshness",
 "navigationUrls": [
  "/**",
  "!/**/.*",
  "!/**/*__*",
  "!/**/*__*/**",
  "!/api/**",
  "!/request-company/**"
 ]
}

