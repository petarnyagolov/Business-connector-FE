{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "appData": {
    "version": "1.2.5",
    "timestamp": "2026-01-09T10:15:00Z",
    "description": "Force update for CSP connect-src fixes"
  },
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
  {
    "name": "static-api",
    "urls": [
      "https://api.xdealhub.com/utils/countries",
      "https://api.xdealhub.com/utils/industries"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 20,
      "maxAge": "7d",
      "timeout": "5s"
    }
  },
  {
    "name": "api-data",
    "urls": [
      "https://api.xdealhub.com/request-company/**",
      "https://api.xdealhub.com/companies/**",
      "https://api.xdealhub.com/user/companies/**",
      "https://api.xdealhub.com/saved-requests/**"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 100,
      "maxAge": "1h",
      "timeout": "10s"
    }
  },
  {
    "name": "user-content",
    "urls": [
      "https://api.xdealhub.com/files/**"
    ],
    "cacheConfig": {
      "strategy": "performance",
      "maxSize": 50,
      "maxAge": "3d",
      "timeout": "15s"
    }
  },
  {
    "name": "external-analytics",
    "urls": [
      "https://www.googletagmanager.com/**",
      "https://*.google-analytics.com/**",
      "https://*.analytics.google.com/**"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 15,
      "maxAge": "1d",
      "timeout": "5s"
    }
  },
  {
    "name": "google-fonts",
    "urls": [
      "https://fonts.googleapis.com/**",
      "https://fonts.gstatic.com/**"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 30,
      "maxAge": "30d",
      "timeout": "10s"
    }
  },
  {
    "name": "external-libraries",
    "urls": [
      "https://cdnjs.cloudflare.com/**"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 10,
      "maxAge": "30d",
      "timeout": "10s"
    }
  }
 ]
}

